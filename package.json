{
  "name": "@filesystem/core",
  "private": true,
  "scripts": {
    "dev": "cross-env node_options=--max_old_space_size=4096 rollup -w -c build/rollup.config.js",
    "build": "cross-env node_options=--max_old_space_size=4096 node build/build-pkg.js",
    "lint": "eslint packages --ext .ts --fix",
    "format": "prettier --write \"+(packages)/**/*.ts\"",
    "test": "mocha",
    "coverage": "nyc mocha",
    "coverage:watch": "nyc mocha --watch",
    "generate-code": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node -r tsconfig-paths/register packages/scripts/src/index.ts",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/chai": "^4.2.9",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "brotli": "^1.3.2",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.2",
    "execa": "^4.0.3",
    "lerna": "^3.22.0",
    "lint-staged": "^10.2.11",
    "mocha": "^7.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sinon": "^9.0.3",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2",
    "zlib": "^1.0.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lywzx/pdd-node-sdk.git"
  },
  "keywords": [
    "pdd",
    "pinduoduo",
    "duoduoke",
    "pdd-sdk",
    "sdk",
    "拼多多",
    "多多客",
    "拼多多SDK"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lywzx/node-js-filesystem/issues"
  },
  "homepage": "https://github.com/lywzx/node-js-filesystem#readme",
  "author": {
    "name": "liuyang",
    "email": "liuyang@lyblog.net"
  }
}
